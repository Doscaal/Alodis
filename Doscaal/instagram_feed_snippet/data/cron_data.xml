<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="instagram_refresh_long_lived_token_cron" model="ir.cron">
            <field name="name">Instagram Feed Snippet: Refresh Long-Lived Token</field>
            <field name="model_id" ref="model_website" />
            <field name="state">code</field>
            <field name="code">model._refresh_instagram_long_lived_token()</field>
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False" />
        </record>
    </data>
</odoo>