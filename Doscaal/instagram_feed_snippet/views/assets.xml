<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="assets_backend_instagram_feed_snippet" inherit_id="web.assets_backend" name="Insagram Feed Snippet Backend Assets">
        <xpath expr="." position="inside">
            <link rel="stylesheet" href="/instagram_feed_snippet/static/src/scss/backend.scss" />
        </xpath>
    </template>
   
    <template id="assets_frontend" inherit_id="website.assets_frontend">
        <xpath expr="link[last()]" position="after">
            <link rel="stylesheet" href="/instagram_feed_snippet/static/src/scss/custom.scss" />
            <link rel="stylesheet" type="text/css" href="/instagram_feed_snippet/static/src/css/owl.carousel.min.css" />
            <link rel="stylesheet" type="text/css" href="/instagram_feed_snippet/static/src/css/owl.theme.default.min.css" />
        </xpath>
        <xpath expr="script[last()]" position="after">
            <script type="text/javascript" src="/instagram_feed_snippet/static/src/js/instafeed.js"></script>
            <script type="text/javascript" src="/instagram_feed_snippet/static/src/js/script.js"></script>
            <script type="text/javascript" src="/instagram_feed_snippet/static/src/js/owl.carousel.min.js"></script>
        </xpath>
    </template>
</odoo>
 